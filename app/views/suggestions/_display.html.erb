<% if @zip != nil%>
<% @zip.map do |pair|%>
    <div class="panel panel-primary">
      <table class="table">
        <tr>
          <td>
          <h3><%= pair[0].name %></h3>
          <br>
          <strong>Type : </strong><%= pair[0].type %>
          <br>
          <%= image_tag pair[0].image, alt: pair[0].image %>
          </td>


          <td>
          <%= button_to "Favorite!", suggestions_favorite_path, method: :post, class: "btn btn-primary" %>
          <iframe src="https://embed.spotify.com/?uri=<%= pair[0].uri %>" frameborder="0" allowtransparency="true"></iframe>
          <h3>Charles ID: <%= pair[2] %></h3>

          </td>


          <td>
          <%= image_tag pair[1].the_image_url, alt: pair[1].the_image_url, class: "yelp_pic" %>
          <h3><%= pair[1].name %></h3>
          <br>
          <strong>Rating : </strong><%=  pair[1].rating%>
          <br>
          <%= image_tag pair[1].rating_img_url_large, alt: pair[1].rating_img_url_large %>
          <br>
          <strong>Total Reviews: </strong><%=  pair[1].review_count%>
          <br>
          <strong>Location: </strong><%  pair[1].location.map do |loc| %>
            <%= loc  %>
            <br>
          <%end %>
          <br>

          <strong>Categories : </strong><% pair[1].categories.map do |cat| %>
            <%= cat[0]  %>
            <br>
          <%end %>
          </td>

        </tr>
      </table>
    </div>

<%end%>
<%end%>
